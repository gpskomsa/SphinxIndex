{
    "name": "gpskomsa/sphinx-index",
    "description": "A zf2 module as a tool to build source data for Sphinx index",
    "type": "library",
    "keywords": [
        "zf2",
        "sphinx"
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-config": "^3.2",
        "zendframework/zend-eventmanager": "^3.2",
        "zendframework/zend-stdlib": "^3.2",
        "zendframework/zend-servicemanager": "^3.4",
        "zendframework/zend-filter": "^2.9",
        "symfony/yaml": "2.*",
        "gpskomsa/sphinx-config": "dev-master#HEAD"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0.1",
        "zfcampus/zf-development-mode": "^3.1",
        "zendframework/zend-test": "^3.0"
    },
    "autoload": {
        "psr-0": {
            "SphinxIndex": "src/"
        },
        "classmap": [
            "./Module.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "SphinxIndexTest\\": "test/SphinxIndexTest/"
        }
    },
    "scripts": {
        "development-disable": "zf-development-mode disable",
        "development-enable": "zf-development-mode enable",
        "development-status": "zf-development-mode status",
        "test": "phpunit --colors=always --testdox"
    }
}
